{
  "name": "music-player-electron",
  "version": "1.0.0",
  "productName": "Music Player",
  "description": "Music Player with Electron",
  "main": "src/main/index.js",
  "config": {
    "electronPackagerIgnore": "^/(src/renderer|config|(?!(|src|dist|package\\.json|LICENSE|node_modules)))"
  },
  "scripts": {
    "build": "npm run clean && npm run webpack",
    "build-dev": "npm run clean && cross-env NODE_ENV=dev npm run webpack",
    "clean": "find . -name '.DS_Store' | xargs rm -f ; rm -rf dist",
    "lint": "echo TODO: Missing Lints",
    "package-mac": "npm run test && npm run build && electron-packager . --platform=darwin --ignore=$npm_package_config_electronPackagerIgnore --out=products --icon=src/_images/logo.png.icns --asar",
    "package-win": "npm run test && npm run build && electron-packager . --platform=win32 --ignore=$npm_package_config_electronPackagerIgnore --out=products --icon=src/_images/logo.ico --asar",
    "start": "electron .",
    "start-dev": "cross-env NODE_ENV=dev electron .",
    "test": "NODE_ENV=dev karma start",
    "webpack": "webpack --config config/webpack.config",
    "webpack-dev-server": "cross-env NODE_ENV=dev webpack-dev-server --config config/webpack.config"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "dot-prop": "^4.2.0",
    "electron-is-dev": "^0.3.0",
    "fs-extra": "^4.0.1",
    "lodash": "^4.17.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-input-range": "^1.2.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/dot-prop": "^4.1.0",
    "@types/enzyme": "^2.8.9",
    "@types/fs-extra": "^4.0.0",
    "@types/lodash": "^4.14.72",
    "@types/mocha": "^2.2.41",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.2",
    "@types/sinon": "^2.3.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "electron": "^1.6.12",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.2.1",
    "karma-electron-launcher": "^0.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.2",
    "webfonts-loader": "^2.0.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erwingo/music-player-electron.git"
  },
  "keywords": [
    "music",
    "player",
    "electron"
  ],
  "author": "Erwin Gaitan O <erwingaitano@gmail.com>",
  "bugs": {
    "url": "https://github.com/erwingo/music-player-electron/issues"
  },
  "homepage": "https://github.com/erwingo/music-player-electron#readme"
}
