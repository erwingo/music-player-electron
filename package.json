{
  "name": "music-player-electron",
  "version": "1.0.0",
  "productName": "Music Player",
  "description": "Music Player with Electron",
  "main": "index.js",
  "config": {
    "electronPackagerIgnore": "^/(src/app|(?!(src|dist|index.js|LICENSE|node_modules)))"
  },
  "scripts": {
    "build": "npm run clean && npm run webpack",
    "clean": "rm -rf dist",
    "dev": "NODE_ENV=development electron .",
    "lint": "echo TODO: Missing Lints",
    "package-mac": "npm run test && npm run build && electron-packager . --platform=darwin --arch=x64 --ignore=$npm_package_config_electronPackagerIgnore --out=products --icon=src/_images/logo.png.icns",
    "start": "electron .",
    "test": "echo TODO: Missing Tests",
    "webpack": "webpack -p --config webpack.config.prod",
    "webpack-dev-server": "webpack-dev-server --config webpack.config.dev"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "dot-prop": "^4.2.0",
    "fs-extra": "^4.0.1",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-input-range": "^1.2.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/dot-prop": "^4.1.0",
    "@types/fs-extra": "^4.0.0",
    "@types/lodash": "^4.14.72",
    "@types/react": "^16.0.2",
    "@types/react-dom": "^15.5.2",
    "css-loader": "^0.28.4",
    "electron": "^1.6.12",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "webfonts-loader": "^2.0.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erwingo/music-player-electron.git"
  },
  "keywords": [
    "music",
    "player",
    "electron"
  ],
  "author": "Erwin Gaitan O <erwingaitano@gmail.com>",
  "bugs": {
    "url": "https://github.com/erwingo/music-player-electron/issues"
  },
  "homepage": "https://github.com/erwingo/music-player-electron#readme"
}
